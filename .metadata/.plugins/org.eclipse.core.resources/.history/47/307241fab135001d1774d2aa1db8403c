package loopDemo;

public class Main {

	public static void main(String[] args) {
		//FOR
		for(int i=1;i<10;i+=2)
		{
			System.out.println(i);
		}
		System.out.println("For Döngüsü bitti");
		
		
		//While
		int i=1;
		while(i<10) {
			System.out.println(i);
			i+=2;
		}
		System.out.println("While Döngüsü bitti");
		//Do-While
		int j=1;
		do {
			System.out.println(i);
			j += 2;
		}while(j<10);
		
	}

}
