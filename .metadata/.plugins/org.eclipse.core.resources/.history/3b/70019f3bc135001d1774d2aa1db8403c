package miniProjeAsalSayi;

public class Main {

	public static void main(String[] args) {
		int number = 29;
		boolean asalmi = true;
		for(int i=2;i<number;i++)
		{
			if(number % i == 0) {
				asalmi = false;
			}
		}
		
		if(asalmi)
		{
			System.out.println(number + "asal sayıdır");
		}
		else 
		{
			System.out.println( number + "asal değildir");
		}


	}
	

}
